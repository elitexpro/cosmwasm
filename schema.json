*** Params ***
{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Params",
  "type": "object",
  "required": [
    "block",
    "contract",
    "message"
  ],
  "properties": {
    "block": {
      "$ref": "#/definitions/BlockInfo"
    },
    "contract": {
      "$ref": "#/definitions/ContractInfo"
    },
    "message": {
      "$ref": "#/definitions/MessageInfo"
    }
  },
  "definitions": {
    "BlockInfo": {
      "type": "object",
      "required": [
        "chain_id",
        "height",
        "time"
      ],
      "properties": {
        "chain_id": {
          "type": "string"
        },
        "height": {
          "type": "integer",
          "format": "int64"
        },
        "time": {
          "type": "integer",
          "format": "int64"
        }
      }
    },
    "Coin": {
      "type": "object",
      "required": [
        "amount",
        "denom"
      ],
      "properties": {
        "amount": {
          "type": "string"
        },
        "denom": {
          "type": "string"
        }
      }
    },
    "ContractInfo": {
      "type": "object",
      "required": [
        "address",
        "balance"
      ],
      "properties": {
        "address": {
          "type": "string"
        },
        "balance": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "$ref": "#/definitions/Coin"
          }
        }
      }
    },
    "MessageInfo": {
      "type": "object",
      "required": [
        "sent_funds",
        "signer"
      ],
      "properties": {
        "sent_funds": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "$ref": "#/definitions/Coin"
          }
        },
        "signer": {
          "type": "string"
        }
      }
    }
  }
}

*** ContractResult ***
{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ContractResult",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "ok"
      ],
      "properties": {
        "ok": {
          "$ref": "#/definitions/Response"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "err"
      ],
      "properties": {
        "err": {
          "type": "string"
        }
      }
    }
  ],
  "definitions": {
    "Coin": {
      "type": "object",
      "required": [
        "amount",
        "denom"
      ],
      "properties": {
        "amount": {
          "type": "string"
        },
        "denom": {
          "type": "string"
        }
      }
    },
    "CosmosMsg": {
      "anyOf": [
        {
          "type": "object",
          "required": [
            "send"
          ],
          "properties": {
            "send": {
              "type": "object",
              "required": [
                "amount",
                "from_address",
                "to_address"
              ],
              "properties": {
                "amount": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/Coin"
                  }
                },
                "from_address": {
                  "type": "string"
                },
                "to_address": {
                  "type": "string"
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "contract"
          ],
          "properties": {
            "contract": {
              "type": "object",
              "required": [
                "contract_addr",
                "msg",
                "send"
              ],
              "properties": {
                "contract_addr": {
                  "type": "string"
                },
                "msg": {
                  "type": "string"
                },
                "send": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/Coin"
                  }
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "opaque"
          ],
          "properties": {
            "opaque": {
              "type": "object",
              "required": [
                "data"
              ],
              "properties": {
                "data": {
                  "type": "string"
                }
              }
            }
          }
        }
      ]
    },
    "Response": {
      "type": "object",
      "required": [
        "data",
        "log",
        "messages"
      ],
      "properties": {
        "data": {
          "type": [
            "string",
            "null"
          ]
        },
        "log": {
          "type": [
            "string",
            "null"
          ]
        },
        "messages": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/CosmosMsg"
          }
        }
      }
    }
  }
}

